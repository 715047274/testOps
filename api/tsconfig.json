{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "sourceMap": false,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "noEmit": true
  },
  "include": ["**/*.ts"]
}
