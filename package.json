{
  "name": "trpc-prisma-electron",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "main/dist/index.cjs",
  "scripts": {
    "watch:web": "ts-node scripts/watchWeb.ts",
    "watch:desktop": "ts-node scripts/watchDesktop.ts",
    "build": "npm run build:main & npm run build:preload & npm run build:renderer",
    "build:main": "cd ./main && tsc && vite build",
    "build:preload": "cd ./preload && tsc && vite build",
    "build:renderer": "cd ./renderer && tsc && vite build",
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development node scripts/watch.mjs",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.mjs && cd ./prisma && prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "electron": "^20.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@tanstack/react-query": "^4.3.9",
    "@trpc/client": "^10.0.0-proxy-beta.0",
    "@trpc/react": "^10.0.0-proxy-beta.0",
    "@trpc/server": "^10.0.0-proxy-beta.0",
    "@vitejs/plugin-react": "^2.1.0",
    "prisma": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.19.1"
  }
