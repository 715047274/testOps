// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output = "../generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model Example {
  id String @id @default(uuid())
}
